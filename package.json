{
  "name": "serverless-plugin-iam-checker",
  "description": "A serverless plugin to check IAM policies for security compliance",
  "version": "0.0.28",
  "scripts": {
    "publish-please": "publish-please",
    "prepublishOnly": "rimraf dist/ && npm run build && publish-please guard",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "istanbul cover node_modules/cucumber/bin/cucumber-js src/*.feature",
    "pretest": "tsc -p ./tsconfig.json",
    "posttest": "rimraf src/**/*.js* src/*.js* src/**/**/*.js* src/**/*.d.ts src/*.d.ts src/**/**/*.d.ts",
    "codecov": "codecov -f coverage/*.json"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "chai": "^4.1.2",
    "codecov": "^3.1.0",
    "cucumber": "^4.2.1",
    "cucumber-tsflow": "^2.2.0",
    "istanbul": "^0.4.5",
    "publish-please": "^3.2.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manwaring/serverless-plugin-iam-checker.git"
  },
  "bugs": {
    "url": "https://github.com/manwaring/serverless-plugin-iam-checker"
  },
  "homepage": "https://github.com/manwaring/serverless-plugin-iam-checker",
  "main": "dist/iam-checker.js",
  "types": "dist/iam-checker.d.ts",
  "author": "From Phillip with â™¥",
  "license": "MIT",
  "keywords": [
    "serverless",
